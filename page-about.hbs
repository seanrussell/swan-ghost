{{!< default}}

<main class="content" role="main">

    <article class="{{post_class}} animated" data-fx="fadeInUp">

        {{#post}}

        <header>
            <h1 class="post-title">{{{title}}}</h1>
        </header>

        <section class="post-content">
            {{content}}
        </section>

        {{/post}}

    </article>

</main>
